{
  "coc": {
    "preferences": {
      "bracketEnterImprove": false,
      "enableFloatHighlight": false,
      "extensionUpdateCheck": "never",
      "formatOnSaveFiletypes": [
        "css",
        "dhall",
        "elixir",
        "javascript",
        "javascriptreact",
        "terraform",
        "tf"
      ],
      "hoverTarget": "preview",
      "previewAutoClose": false,
      "diagnostic": {
        "checkCurrentLine": true
      },
      "snippets": {
        "enable": false
      }
    }
  },
  "signature": {
    "enable": false
  },
  "suggest": {
    "autoTrigger": "none"
  },
  "languageserver": {
    "elixir": {
      "command": "~/code/elixir-ls/release/language_server.sh",
      "filetypes": ["elixir", "eelixir"],
      "rootPatterns": ["mix.exs"]
    },
		"terraform": {
			"command": "~/.bin/terraform-lsp/terraform-lsp",
			"args": ["serve"],
			"filetypes": [
				"terraform",
				"tf"
			],
			"initializationOptions": {},
			"settings": {}
		},
		"dhall": {
      "command": "dhall-lsp-server",
      "filetypes": ["dhall"],
      "initializationOptions": {
        "vscode-dhall-lsp-server": {
          "asciiOnly": true
        }
      }
    }
  },
  "list": {
    "insertMappings": {
      "<C-n>": "normal:j",
      "<C-p>": "normal:k"
    },
    "normalMappings": {
      "<C-n>": "normal:j",
      "<C-p>": "normal:k"
    }
  },
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "prettier.disableSuccessMessage": true
}
